{
  "name": "yn-mobile-endpoint",
  "version": "2.0.0",
  "description": "REST service for YouNeed mobile environment",
  "scripts": {
    "serve": "nodemon dist/main.js",
    "build": "npm run build-ts && npm run tslint",
    "build-ts": "tsc",
    "watch": "tsc -w",
    "serve-debug": "nodemon --inspect dist/main.js",
    "watch-debug": "concurrently -k \"npm run watch\" \"npm run serve-debug\"",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest --watch",
    "fs-serve": "nodemon dist/fs-main.js",
    "fs-serve-debug": "nodemon --inspect dist/fs-main.js",
    "fs-watch-debug": "concurrently -k \"npm run watch\" \"npm run fs-serve-debug\""
  },
  "jest": {
    "globals": {
      "tsConfigFile": "tsconfig.json"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "chalk": "^2.4.2",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "handlebars": "^4.1.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "mime-types": "^2.1.24",
    "moment": "^2.24.0",
    "mongodb": "^3.2.6",
    "pg": "^7.11.0",
    "randomstring": "^1.1.5",
    "sequelize": "^5.8.6",
    "sequelize-butler": "^2.0.6",
    "sparkpost": "^2.1.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.27",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.132",
    "@types/mime-types": "^2.1.0",
    "@types/moment": "^2.13.0",
    "@types/mongodb": "^3.1.27",
    "@types/node": "^12.0.3",
    "@types/randomstring": "^1.1.6",
    "@types/sparkpost": "^2.1.4",
    "@types/uuid": "^3.4.4",
    "@types/validator": "^10.11.0",
    "ts-node": "^8.2.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/ambrogio-dev/yn-mobile-endpoint.git"
  },
  "author": "Ambrogio S.r.l.",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/ambrogio-dev/yn-mobile-endpoint/issues"
  },
  "homepage": "https://gitlab.com/ambrogio-dev/yn-mobile-endpoint#README"
}
