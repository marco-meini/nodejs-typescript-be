{
  "name": "yn-mobile-endpoint",
  "version": "1.0.0",
  "description": "REST service for YouNeed mobile environment",
  "scripts": {
    "serve": "nodemon dist/main.js",
    "build": "npm run build-ts && npm run tslint",
    "build-ts": "tsc",
    "watch": "tsc -w",
    "serve-debug": "nodemon --inspect dist/main.js",
    "watch-debug": "concurrently -k \"npm run watch\" \"npm run serve-debug\"",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest --watch",
    "fs-serve": "nodemon dist/fs-main.js",
    "fs-serve-debug": "nodemon --inspect dist/fs-main.js",
    "fs-watch-debug": "concurrently -k \"npm run watch\" \"npm run fs-serve-debug\""
  },
  "jest": {
    "globals": {
      "tsConfigFile": "tsconfig.json"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "async": "^2.6.0",
    "bcrypt": "^2.0.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "chalk": "^2.3.2",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "fs-extra": "^5.0.0",
    "handlebars": "^4.0.11",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.5",
    "mime-types": "^2.1.18",
    "moment": "^2.20.1",
    "path": "^0.12.7",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "randomstring": "^1.1.5",
    "redis": "^2.8.0",
    "sequelize": "^4.33.3",
    "sequelize-butler": "^2.0.1",
    "sparkpost": "^2.1.2",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/chai": "^4.1.2",
    "@types/cookie": "^0.3.1",
    "@types/express": "^4.11.1",
    "@types/handlebars": "^4.0.36",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/mime-types": "^2.1.0",
    "@types/randomstring": "^1.1.6",
    "@types/redis": "^2.8.5",
    "@types/sequelize": "^4.27.5",
    "@types/sparkpost": "^2.1.3",
    "@types/supertest": "^2.0.4",
    "@types/jsonwebtoken": "^7.2.6",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "jest": "^22.4.3",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/ambrogio-dev/yn-mobile-endpoint.git"
  },
  "author": "Ambrogio S.r.l.",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/ambrogio-dev/yn-mobile-endpoint/issues"
  },
  "homepage": "https://gitlab.com/ambrogio-dev/yn-mobile-endpoint#README"
}
