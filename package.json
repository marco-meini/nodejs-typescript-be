{
  "name": "yn-mobile-endpoint",
  "version": "2.0.0",
  "description": "REST service for YouNeed mobile environment",
  "scripts": {
    "serve": "nodemon dist/main.js",
    "build": "npm run build-ts && npm run tslint",
    "build-ts": "tsc",
    "watch": "tsc -w",
    "serve-debug": "nodemon --inspect dist/main.js",
    "watch-debug": "concurrently -k \"npm run watch\" \"npm run serve-debug\"",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest --watch",
    "fs-serve": "nodemon dist/fs-main.js",
    "fs-serve-debug": "nodemon --inspect dist/fs-main.js",
    "fs-watch-debug": "concurrently -k \"npm run watch\" \"npm run fs-serve-debug\""
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "chalk": "^4.0.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "handlebars": "^4.7.6",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mime-types": "^2.1.27",
    "moment": "^2.25.3",
    "mongodb": "^3.5.7",
    "pg": "^8.1.0",
    "randomstring": "^1.1.5",
    "sequelize": "^5.21.8",
    "sequelize-butler": "^2.0.8",
    "sparkpost": "^2.1.4",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/lodash": "^4.14.150",
    "@types/mailgun-js": "^0.22.4",
    "@types/mime-types": "^2.1.0",
    "@types/mongodb": "^3.5.16",
    "@types/node": "^13.13.5",
    "@types/randomstring": "^1.1.6",
    "@types/uuid": "^7.0.3",
    "chai": "^4.2.0",
    "jest": "^26.0.1",
    "mailgun-js": "^0.22.0",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/ambrogio-dev/yn-mobile-endpoint.git"
  },
  "author": "Ambrogio S.r.l.",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/ambrogio-dev/yn-mobile-endpoint/issues"
  },
  "homepage": "https://gitlab.com/ambrogio-dev/yn-mobile-endpoint#README"
}
